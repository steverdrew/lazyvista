- content_for :javascript_includes do
  = javascript_include_tag "properties/general.js"
  = javascript_include_tag "properties/edit_form.js"   
#overlay
#preloader
  #status
.row
  .small-12.large-12.columns
    #general_messages
      = render "layouts/messages"
    %p
      %a{name: "general"}
    %br/
    %h4.subheader.magellan-menu{"data-magellan-destination"=>"general"} General
.row
  #screen
    .small-3.large-3.columns
      #promo_image
        = render "promo_image" 
    .small-9.large-9.columns
      #general
        = render "general"
.row
  #screen
    .small-12.large-12.columns
      %p
        %a{name: "location"}
      %br/
      %h4.subheader.magellan-menu{"data-magellan-destination"=>"location"} Location
      
      #location_messages
        = render "layouts/messages"
      #location
        = render "location"
.row
  #screen
    .small-12.large-12.columns
      %p
        %a{name: "gallery"}
      %br/  
      %h4.subheader.magellan-menu{"data-magellan-destination"=>"gallery"} Gallery
      /= render "gallery"
.row
  #screen
    .small-12.large-12.columns
      %p
        %a{name: "features"}
      %br/  
      %h4.subheader.magellan-menu{"data-magellan-destination"=>"features"} Features
      /= render "features"
        
.row
  #screen
    .small-12.large-12.columns
      %p
        %a{name: "descriptions"}
      %br/  
      %h4.subheader.magellan-menu{"data-magellan-destination"=>"descriptions"} Descriptions
      /= render "descriptions"